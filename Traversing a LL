class Node:
    //Node of a list
    def __init__(self,value):
        self.data = value
        self.next = None
        
class LinkedList:
    //Head of LL
    def __init__(self):
        self.head = None
        self.n = 0

    //Length of LL
    def __len__(self):
      return self.n

    //Insert a node from head
    def insertFromHead(self,value):
        new_node = Node(value)
        new_node.next = self.head
        self.head = new_node
        self.n += 1

    #  Traverse the LL
    def traverse(self):
        curr = self.head
        while curr != None:
            print(curr.data)
            curr = curr.next
            
L = LinkedList()
L.insertFromHead(1)
L.insertFromHead(2)
L.insertFromHead(3)
L.insertFromHead(4)

L.traverse()
