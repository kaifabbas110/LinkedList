class Node:
    //Node of a list
    def __init__(self,value):
        self.data = value
        self.next = None
        
class LinkedList:
    //Head of LL
    def __init__(self):
        self.head = None
        self.n = 0

    //Length of LL
    def __len__(self):
      return self.n

    //Insert a node on head
    def insertHead(self,value):
        new_node = Node(value)
        new_node.next = self.head
        self.head = new_node
        self.n += 1
L = LinkedList()
L.insertHead(1)
L.insertHead(2)
L.insertHead(3)
